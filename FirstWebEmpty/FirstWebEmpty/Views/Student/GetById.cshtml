
@model Student
<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Name</th>
                <th scope="col">Age</th>
            </tr>
        </thead>
        <tbody>
                <tr>
                    <td>@Model.Id</td>
                    <td>@Model.Name</td>
                    <td>@Model.Age</td>
                </tr>
        </tbody>
    </table>
    <div>
        <a  asp-controller="home" asp-action="index" class="btn btn-warning">Go back home</a>
    </div>
</div>

