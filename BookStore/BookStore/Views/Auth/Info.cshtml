<link rel="stylesheet" href="~/Css/login.css">
@model AppUser
<!-- protected page txt start -->
<div class="bg-blue">
    <div class="nav container d-flex">
        <h1 class="unique-text">Protected Page</h1>
        <p class="gray w-400">
            There are many variations of passages of Lorem Ipsum available, have
            suffered alteration in some form.
        </p>
    </div>
</div>
<!-- protected page txt end -->
<!-- Login start -->
<div class="login-container d-flex center">
    <div class="bg-light-gray d-flex f-dir-column">
        <div class="col-md-6 col-md-offset-3 text-center">
            <div class="login">
                <div class="blue protected-form-heading">
                    <div class="login-text">
                        <h2 class="blue center">Information about user</h2>
                        <span class="gray">Please register using account detail bellow.</span>
                    </div>
                    <div class="login-form">
                        <form method="post">
                            <div style="color:black">
                                <label asp-for="FirstName"></label>
                                <input type="text" asp-for="FirstName">
                            </div>
                            <div style="color:black">
                                <label asp-for="LastName"></label>
                                <input type="text" asp-for="LastName">
                            </div>
                            <div style="color:black">
                                <label asp-for="UserName"></label>
                                <input type="text" asp-for="UserName">
                            </div>
                            <div style="color:black">
                                <label asp-for="Email"></label>
                                <input type="email" asp-for="Email">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>